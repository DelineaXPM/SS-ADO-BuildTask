{
    "manifestVersion": 1,
    "id": "cloudkitects-ss-task",
    "name": "Cloudkitects Secret Server Azure DevOps Task",
    "version": "1.0.0",
    "publisher": "CLoudkitects",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Cloudkitects Azure DevOps task for accessing secrets from Delinea Secret Server",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "files": [
        {
            "path": "DSSV1"
        }
    ],
	"content": {
		"details": {
			"path": "overview.md"
		},
		"license": {
			"path": "LICENSE"
		}
	},
    "links": {
        "home": {
            "uri": "https://cloudkitects.com"
        },
        "learn": {
            "uri": "https://cloudkitects.com"
        },
        "support": {
            "uri": "https://cloudkitects.com/support/"
        },
        "repository": {
            "uri": "https://github.com/cloudkitects/SS-ADO-BuildTask"
        },
        "issues": {
            "uri": "https://github.com/cloudkitects/SS-ADO-BuildTask/issues"
        }
    },
    "contributions": [
        {
            "id": "cloudkitects-ss-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "DSSV1"
            }
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/cloudkitects/SS-ADO-BuildTask"
    },
    "galleryFlags": [
        "Public"
    ]
}
